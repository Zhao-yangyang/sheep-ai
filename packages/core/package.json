    "next": "^16.1.1",
    "nuqs": "^2.8.6",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "undici-types": "^7.18.0"
